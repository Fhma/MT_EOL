<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="filesystem" nsURI="filesystem" nsPrefix="filesystem">
  <eClassifiers xsi:type="ecore:EClass" name="Filesystem">
    <eAnnotations source="https://eclipse.org/epsilon/incremental/equals" references="//Filesystem/drives"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="drives" upperBound="-1" eType="//Drive" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="syncs" upperBound="-1" eType="//Sync" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Drive" eSuperTypes="//Folder">
    <eAnnotations source="https://eclipse.org/epsilon/incremental/OrientDbIndex" references="//File/name">
      <details key="type" value="NOTUNIQUE_HASH_INDEX"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Folder" eSuperTypes="//File">
    <eAnnotations source="https://eclipse.org/epsilon/incremental/equals" references="//Folder/contents"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="contents" upperBound="-1" eType="//File" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Shortcut" eSuperTypes="//File">
    <eAnnotations source="https://eclipse.org/epsilon/incremental/equals" references="//Shortcut/target"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="//File"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Sync">
    <eAnnotations source="https://eclipse.org/epsilon/incremental/equals" references="//Sync/source"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="//File"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="//File">
      <eAnnotations source="https://eclipse.org/epsilon/incremental/Graph">
        <details key="edge" value="true"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lastSync">
      <eAnnotations source="https://eclipse.org/epsilon/incremental/Graph">
        <details key="edge" value="true"/>
      </eAnnotations>
      <eType xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="File">
    <eAnnotations source="https://eclipse.org/epsilon/incremental/OrientDbIndex" references="//File/name">
      <details key="type" value="NOTUNIQUE_HASH_INDEX"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name">
      <eType xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
  </eClassifiers>
