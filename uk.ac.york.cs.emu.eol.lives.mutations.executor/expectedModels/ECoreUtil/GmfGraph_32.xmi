<?xml version="1.0" encoding="ASCII"?>
<gmfgraph:Canvas xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition" name="widgets">
  <figures name="Default">
    <descriptors name="WidgetFigure">
      <actualFigure xsi:type="gmfgraph:ScalablePolygon" name="WidgetFigure">
        <border xsi:type="gmfgraph:MarginBorder">
          <insets top="5" left="5" bottom="5" right="5"/>
        </border>
        <children xsi:type="gmfgraph:Label" name="WidgetLabelFigure" text="Widget"/>
        <template x="1" y="6"/>
        <template x="2" y="7"/>
        <template x="3" y="8"/>
        <template x="4" y="9"/>
        <template x="5" y="10"/>
      </actualFigure>
      <accessors accessor="getFigureWidgetLabelFigure" figure="//@figures.0/@descriptors.0/@actualFigure/@children.0"/>
    </descriptors>
  </figures>
  <nodes name="Widget" figure="WidgetFigure"/>
  <labels name="WidgetLabel" figure="WidgetFigure" elementIcon="false" accessor="//@figures.0/@descriptors.0/@accessors.0"/>
</gmfgraph:Canvas>
