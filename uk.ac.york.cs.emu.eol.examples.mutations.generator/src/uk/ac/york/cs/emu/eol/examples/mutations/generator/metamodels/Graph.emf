@namespace(uri="Graph", prefix="graph")

package graph;

class Graph {
  val GraphElement[*]#graph contents;
}

abstract class GraphElement {
  ref Graph#contents graph;
}

class Node extends GraphElement {
  attr String label;
  ref Edge[*] edges;
}
	
class Edge extends GraphElement {
  attr Integer weight;
  ref Node source;
  ref Node target;
}